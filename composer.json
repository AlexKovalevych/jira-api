    "keywords": [
        "api", "git", "github", "jira", "pull request", "contributor", "community", "package"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Kovalevych",
            "email": "alexkovalevych@gmail.com"
        },
